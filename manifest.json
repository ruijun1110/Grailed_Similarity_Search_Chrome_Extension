{
    "manifest_version": 3,
    "name": "Grailed Image Search",
    "description": "Base Level Extension",
    "version": "1.0",
    "permissions": [
        "activeTab"
      ],
    "action": {
      "default_popup": "popup/popup.html",
      "default_icon": "images/store_search.png"
    },
    "content_scripts": [
        {
          "matches": ["https://www.grailed.com/listings/*"],
          "js": ["scripts/content.js"]
        }
      ]
  }